datasource db {
    provider = "postgresql"
    url      = "postgresql://postgres:postgres@localhost:5432/test?sslmode=disable"
}

generator db {
    provider = "go run github.com/prisma/prisma-client-go"
}

model ModelsPrisma {
    id        Int   @id @default(autoincrement())
    name      String
    title     String
    fax       String
    web       String
    age       Int
    right     Boolean
    counter   BigInt
}